import java.util.*;
public class main3{
    public static void findPrime(int a){
        if(a<=1){
            System.out.println("False");
            return;
        }
        for(int i =2; i<=a/2; i++){
            if(a%i == 0){
                System.out.println("False");
                return;
            }
        }
        System.out.println("True");
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();

        findPrime(a);
    }
}
