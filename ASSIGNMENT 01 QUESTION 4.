import java.util.*;
public class main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int n = num;
        int count = 0;
        if(n==0){
            count = 1;
        }
        else{
            while(n!=0){
                n = n/10;
                count++;
            }
        }
        System.out.println(count);
    }
}
