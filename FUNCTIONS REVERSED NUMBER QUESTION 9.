import java.util.*;
public class main3{
    public static void reversedNumber(int n){
        int reversed = 0;
        while(n!=0){
            int digit = n%10;
            reversed = reversed*10 + digit;
            n = n/10;
        }
        System.out.println(reversed);
        return;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        reversedNumber(n);
        
    }
}
