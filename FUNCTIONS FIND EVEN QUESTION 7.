import java.util.*;
public class main3{
    public static void isEven(int a){
        if(a%2==0){
            System.out.println("True");
            
        }
        else{
            System.out.println("False");
            
        }
        return;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();

        isEven(a);
    }
}
